{
  "name": "sfxui",
  "version": "0.2.0",
  "description": "A library of PaperJS user interface components for HTML based DAW's",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "files": [
    "src",
    "lib"
  ],
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js"
  },
  "scripts": {
    "clean": "rimraf lib",
    "declarations": "tsc -p tsconfig.json",
    "build:esm": "cross-env BABEL_ENV=esmUnbundled babel src --extensions '.ts' --out-dir 'lib/esm' --source-maps",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --extensions '.ts' --out-dir 'lib/cjs' --source-maps",
    "build:bundles": "cross-env BABEL_ENV=esmBundled rollup -c",
    "build": "npm-run-all -l clean -p build:esm build:cjs build:bundles declarations",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nogbit/sfxui.git"
  },
  "keywords": [
    "audio",
    "daw",
    "user interface",
    "paper"
  ],
  "author": "Marcellus Miles",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nogbit/sfxui/issues"
  },
  "homepage": "https://github.com/nogbit/sfxui#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@types/jest": "^26.0.20",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.4",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@svgdotjs/svg.js": "^3.1.1"
  }
}
