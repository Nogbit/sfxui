{
  "name": "starter",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "parcel html/dev.html --out-dir build/dist --out-file sfxui.html --global sfxui",
    "test": "parcel html/test.html --port 5678 --out-dir build/test & node test/runner.js && npm run --silent posttest",
    "posttest": "fuser -k -TERM -n tcp 5678 > /dev/null 2>&1 || true",
    "build": "npm run lint && npm run package",
    "lint": "./node_modules/.bin/eslint src/* test/*",
    "package": "parcel build html/dev.html --out-dir build/dist --out-file sfxui.html --global sfxui"
  },
  "keywords": [],
  "author": "",
